ute exact path=\"/\" component={Homepage} />\n              <Route exact path=\"/login\" component={Login} />\n              <Route exact path=\"/signup\" component={Signup} />\n              <Route component={NoMatch} />\n            </Switch>\n          </StoreProvider>\n        </div>\n      </Router>\n    </ApolloProvider>\n  );\n}\n\nexport default App;\n","/Users/suzannaakins/Desktop/bootcamp/projects/petter-life/client/src/pages/Homepage/index.js",["56","57"],"import React from 'react';\nimport Header from '../../components/Header';\nimport Footer from '../../components/Footer';\nimport Signup from '../Signup';\nimport Login from '../Login';\n\nfunction Homepage() {\n  return (\n    <div>\n      <Header/>\n      <Footer/>\n    </div>\n  );\n}\n\nexport default Homepage;\n","/Users/suzannaakins/Desktop/bootcamp/projects/petter-life/client/src/pages/Login/index.js",["58"],"import React, { useState } from \"react\";\nimport { useMutation } from '@apollo/react-hooks';\nimport { Link } from \"react-router-dom\";\nimport { LOGIN } from \"../../utils/mutations\"\nimport Auth from \"../../utils/auth\";\nimport './index.css';\n\nfunction Login(props) {\n  const [formState, setFormState] = useState({ username: '', password: '' })\n  const [login, { error }] = useMutation(LOGIN);\n\n  const handleFormSubmit = async event => {\n    event.preventDefault();\n    try {\n      const mutationResponse = await login({ variables: { username: formState.username, password: formState.password } })\n      const token = mutationResponse.data.login.token;\n      Auth.login(token);\n    } catch (e) {\n      console.log(e)\n    }\n  };\n\n  const handleChange = event => {\n    const { name, value } = event.target;\n    setFormState({\n      ...formState,\n      [name]: value\n    });\n  };\n\n  return (\n    <div className=\"LoginSection\">\n      <div className=\"LoginContainer\">\n        <div className=\"LoginTitle\">Login</div>\n        <form className=\"LoginCredentials\" onSubmit={handleFormSubmit}>\n          <div className=\"LoginUsername\">\n            <label for=\"username\">username</label>\n            <input \n              className=\"LoginUsernameInputField\" \n              id=\"username\" \n              name=\"username\" \n              type=\"input\" \n              onChange={handleChange}\n            />\n          </div>\n          <div className=\"LoginPassword\">\n            <label for=\"password\">password</label>\n            <input \n              className=\"LoginPasswordInputField\" \n              id=\"pwd\" \n              name=\"password\" \n              type=\"password\" \n              onChange={handleChange} \n            />\n          </div>\n          <button className=\"LoginBtn\" type=\"submit\">Login</button>\n          <Link className=\"SignupLink\" to=\"/signup\">Signup Instead</Link>\n        </form>\n      </div>\n    </div>\n  );\n}\n\nexport default Login;","/Users/suzannaakins/Desktop/bootcamp/projects/petter-life/client/src/pages/Signup/index.js",["59"],"import React from 'react';\nimport { Link } from \"react-router-dom\";\nimport { useMutation } from '@apollo/react-hooks';\nimport Auth from \"../../utils/auth\";\nimport { ADD_USER } from \"../../utils/mutations\";\nimport './index.css';\n\nfunction Signup(props) {\n  const [formState, setFormState] = useState({ username: '', email: '', password: '' });\n  const [addUser] = useMutation(ADD_USER);\n\n  const handleFormSubmit = async event => {\n    event.preventDefault();\n    const mutationResponse = await addUser({\n      variables: {\n        email: formState.email, password: formState.password,\n        firstName: formState.firstName, lastName: formState.lastName\n      }\n    });\n    const token = mutationResponse.data.addUser.token;\n    Auth.login(token);\n  };\n\n  const handleChange = event => {\n    const { name, value } = event.target;\n    setFormState({\n      ...formState,\n      [name]: value\n    });\n  };\n  \n  return (\n    <div className=\"SignupSection\">\n      <div className=\"SignupContainer\">\n        <div className=\"SignupTitle\">Signup</div>\n        <form className=\"SignupCredentials\" onSubmit={handleFormSubmit}>\n          <div className=\"SignupUsername\">\n            <label for=\"username\">username</label>\n            <input \n              className=\"SignupUsernameInputField\" \n              name=\"username\" \n              type=\"input\" \n              onChange={handleChange}\n            />\n          </div>\n          <div className=\"SignupEmail\">\n            <label for=\"email\">email</label>\n            <input className=\"SignupEmailInputField\" name=\"email\" type=\"email\" onChange={handleChange}></input>\n          </div>\n          <div className=\"SignupPassword\">\n            <label for=\"password\">password</label>\n            <input \n              className=\"SignupPasswordInputField\" \n              name=\"password\" \n              type=\"password\" \n              onChange={handleChange}\n            />\n          </div>\n        </form> \n        <button className=\"SignupBtn\" type=\"submit\">Submit</button>\n        <Link className=\"LoginLink\" to=\"/login\">Log In Instead</Link>\n      </div>\n    </div>\n  );\n}\n\nexport default Signup;","/Users/suzannaakins/Desktop/bootcamp/projects/petter-life/client/src/components/Footer/index.js",[],"/Users/suzannaakins/Desktop/bootcamp/projects/petter-life/client/src/components/Header/index.js",[],"/Users/suzannaakins/Desktop/bootcamp/projects/petter-life/client/src/utils/auth.js",[],"/Users/suzannaakins/Desktop/bootcamp/projects/petter-life/client/src/utils/mutations.js",[],{"ruleId":"60","replacedBy":"61"},{"ruleId":"62","replacedBy":"63"},{"ruleId":"64","severity":2,"message":"65","line":6,"column":20,"nodeType":"66","messageId":"67","endLine":6,"endColumn":32},{"ruleId":"68","severity":2,"message":"69","line":20,"column":6,"nodeType":"70","endLine":20,"endColumn":20},{"ruleId":"68","severity":